# Use default setting for mysqld processes
!include include/default_mysqld.cnf

[mysqld.1]
log-bin=                 master-bin
loose-federated

[mysqld.2]
loose-federated

[ENV]
MASTER_MYPORT=           @mysqld.1.port
MASTER_MYSOCK=           @mysqld.1.socket

SLAVE_MYPORT=            @mysqld.2.port
SLAVE_MYSOCK=            @mysqld.2.socket
